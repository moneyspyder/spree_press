<% content_for :page_actions do %>
  <%= button_link_to Spree.t(:back_to_clippings_list), spree.admin_press_clippings_path, class: 'btn-primary', icon: 'chevron-left' %>
  <% if can?(:create, Spree::PressClipping) %>
    <%= button_link_to Spree.t(:new_press_clipping), new_object_url, { class: "btn-success", icon: 'add', id: 'admin_new_press_clipping' } %>
  <% end %>
<% end %>